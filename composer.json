{
  "name": "kozlovsv/yii2-crud",
  "description": "Набор библиотек для быстрого построение CRUD приложений",
  "keywords": [
    "yii2",
    "extension",
    "widget",
    "form",
    "crud"
  ],
  "homepage": "https://github.com/kozlovsv/yii2-crud",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Kozlov Sergey",
      "email": "kozlovsv78@gmail.com",
      "homepage": "https://atonex.ru/"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "yiisoft/yii2-bootstrap": "*",
    "kozlovsv/yii2-widget-datetimepicker": "@dev",
    "kartik-v/yii2-widget-select2": "*",
    "kartik-v/yii2-builder": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url":  "https://github.com/kozlovsv/yii2-widget-datetimepicker.git"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "kozlovsv\\crud\\": ""
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer" : true
    },
    "fxp-asset": {
      "enabled": false
    }
  }
}
